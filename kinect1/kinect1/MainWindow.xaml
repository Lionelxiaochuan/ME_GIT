<Window x:Class="kinect1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="500" Width="700">
    <Grid>
        <Image Name="ColorImage"></Image>
        <Canvas Background="Transparent" Name="SkeletonCanvas">
            <Ellipse Fill="Red" Height="20" Width="20" Name="EllipseLefthand" Stroke="White"></Ellipse>
            <Ellipse Fill="Red" Height="20" Width="20" Name="EllipseRighthand" Stroke="White"></Ellipse>
            <Ellipse Fill="Red" Height="20" Width="20" Name="EllipseHead" Stroke="White"></Ellipse>
        </Canvas>

    </Grid>
</Window>
